{
    "split_char": ";",
    "target_var": "target",
    "regularization": 0,
    "rule_filters": {
        "l1_row_threshold": 0.01,
        "l1_column_threshold": 0.01,
        "dependency_threshold": 0,
        "significance_level": 0.05,
        "remove_insignificant_rules": false,
        "only_whitelist": false,
        "only_one_round_of_statistical_removal": true,
        "only_one_round_of_linearity_removal": true
    },
    "numerical_fuzzification": ["low", "medium", "high"],
    "numerical_defuzzification": ["verylow", "medium", "veryhigh"],
    "variance_threshold": 1e-5,
    "remove_low_variance": false,
    "include_intercept": true,
    "outlier_filtering": {
        "AGE": {
            "method": "VariableBounds",
            "min": 0,
            "max": 100
        },
        "TAX": { 
            "method": "IQR",
            "outlier_iqr_multiplier": 4
        }
    },
    "num_vars": 1,
    "whitelist": [
    ],
    "blacklist": [
        
    ],
    "rule_priority_weights": {
        "support_weight": 1,
        "leverage_weight": 10,
        "num_antecedents_weight": 1,
        "whitelist_boolean_weight": 1000
    }
}
